<?xml version="1.0" encoding="utf8"?>
<shader_source>
 <name>default</name>
 <type>view_transform</type>
 <shader_source_datas>
  <shader_source_data>
   <parameters>
    <parameter>
     <name>world_position</name>
     <source></source>
     <type>vec3</type>
     <qualifier>in</qualifier>
     <size>one</size>
     <scale>1</scale>
    </parameter>
    <parameter>
     <name>view_matrix</name>
     <source></source>
     <type>mat4</type>
     <qualifier>in</qualifier>
     <size>one</size>
     <scale>1</scale>
    </parameter>
    <parameter>
     <name>view_position</name>
     <source></source>
     <type>vec3</type>
     <qualifier>out</qualifier>
     <size>one</size>
     <scale>1</scale>
    </parameter>
   </parameters>
   <source>{
	view_position = vec3(view_matrix * vec4(world_position, 1.0));
}
</source>
   <glsl_120>true</glsl_120>
   <glsl_150>true</glsl_150>
   <material_default>true</material_default>
   <material_texture_arrays>true</material_texture_arrays>
  </shader_source_data>
 </shader_source_datas>
</shader_source>
