<?xml version="1.0" encoding="utf8"?>
<shader_source>
 <name>blend</name>
 <type>diffuse_mapping</type>
 <shader_source_datas>
  <shader_source_data>
   <parameters>
    <parameter>
     <name>fragment_uv</name>
     <source></source>
     <type>vec2</type>
     <qualifier>in</qualifier>
     <size>one</size>
     <scale>1</scale>
    </parameter>
    <parameter>
     <name>diffuse_sampler_0</name>
     <source></source>
     <type>sampler2D</type>
     <qualifier>in</qualifier>
     <size>one</size>
     <scale>1</scale>
    </parameter>
    <parameter>
     <name>diffuse_sampler_1</name>
     <source></source>
     <type>sampler2D</type>
     <qualifier>in</qualifier>
     <size>one</size>
     <scale>1</scale>
    </parameter>
    <parameter>
     <name>vertex_color</name>
     <source></source>
     <type>vec4</type>
     <qualifier>in</qualifier>
     <size>one</size>
     <scale>1</scale>
    </parameter>
    <parameter>
     <name>diffuse</name>
     <source></source>
     <type>vec4</type>
     <qualifier>out</qualifier>
     <size>one</size>
     <scale>1</scale>
    </parameter>
   </parameters>
   <source>{
	diffuse = mix(texture2D(diffuse_sampler_0, fragment_uv), texture2D(diffuse_sampler_1, fragment_uv), vertex_color.a);
}
</source>
   <glsl_120>true</glsl_120>
