<?xml version="1.0" encoding="utf8"?>
<shader_source>
 <name>static_extra</name>
 <type>uv</type>
 <shader_source_datas>
  <shader_source_data>
   <parameters>
    <parameter>
     <name>texture_coordinate_0</name>
     <type>vec4</type>
     <qualifier>in</qualifier>
     <size>one</size>
     <scale>1</scale>
    </parameter>
    <parameter>
     <name>texture_matrices</name>
     <type>mat2x3</type>
     <qualifier>in</qualifier>
     <size>one</size>
     <scale>2</scale>
    </parameter>
    <parameter>
     <name>world_extra_uv</name>
     <type>vec2</type>
     <qualifier>out</qualifier>
     <size>one</size>
     <scale>1</scale>
    </parameter>
    <parameter>
     <name>world_uv</name>
     <type>vec2</type>
     <qualifier>out</qualifier>
     <size>one</size>
     <scale>1</scale>
    </parameter>
   </parameters>
   <source>world_uv = vec3(texture_coordinate_0.xy, 1.0) * texture_matrices[0];
world_extra_uv = vec3(texture_coordinate_0.xy, 1.0) * texture_matrices[1];
</source>
   <version>120</version>
  </shader_source_data>
 </shader_source_datas>
</shader_source>
