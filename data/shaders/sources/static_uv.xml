<?xml version="1.0" encoding="utf8"?>
<shader_source>
 <name>static</name>
 <type>uv</type>
 <shader_source_datas>
  <shader_source_data>
   <parameters>
    <parameter>
     <name>texture_coordinate_0</name>
     <type>vec4</type>
     <qualifier>in</qualifier>
     <size>one</size>
     <scale>1</scale>
    </parameter>
    <parameter>
     <auto_parameter>texture_matrices</auto_parameter>
    </parameter>
    <parameter>
     <common_parameter>world_uv</common_parameter>
     <qualifier>out</qualifier>
    </parameter>
   </parameters>
   <source>world_uv = vec3(texture_coordinate_0.xy, 1.0) * texture_matrices[0];
</source>
   <version>120</version>
  </shader_source_data>
 </shader_source_datas>
</shader_source>
