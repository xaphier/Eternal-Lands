<?xml version="1.0" encoding="utf8"?>
<shader_source>
 <name>static</name>
 <type>uv</type>
 <shader_source_datas>
  <shader_source_data>
   <parameters>
    <parameter>
     <vertex_semantic>texture_coordinate</vertex_semantic>
    </parameter>
    <parameter>
     <auto_parameter>texture_matrices</auto_parameter>
    </parameter>
    <parameter>
     <common_parameter>world_uv</common_parameter>
     <qualifier>out</qualifier>
    </parameter>
   </parameters>
   <source>world_uv = vec3(texture_coordinate.xy, 1.0) * texture_matrices[0];
</source>
   <version>120</version>
  </shader_source_data>
 </shader_source_datas>
</shader_source>
