<?xml version="1.0" encoding="utf8"?>
<shader_source>
 <name>lava</name>
 <types>
  <type>uv</type>
 </types>
 <shader_source_datas>
  <shader_source_data>
   <parameters>
    <parameter>
     <auto_parameter>time</auto_parameter>
    </parameter>
    <parameter>
     <name>uv</name>
     <type>vec2</type>
     <qualifier>out</qualifier>
     <size>one</size>
     <scale>3</scale>
    </parameter>
    <parameter>
     <common_parameter>world_position</common_parameter>
     <qualifier>in</qualifier>
    </parameter>
   </parameters>
   <source>uv[0] = world_position.xy * 0.1 + vec2(-2.5, -1.0) * time * 0.01;
uv[1] = world_position.xy * 0.15 + vec2(1.5, -1.5) * time * 0.02;
uv[2] = world_position.xy * 0.2 + vec2(-0.5, 2.0) * time * 0.015;
</source>
   <version>120</version>
  </shader_source_data>
 </shader_source_datas>
</shader_source>
